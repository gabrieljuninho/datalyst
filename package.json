{
  "name": "datalyst",
  "description": "A comprehensive package designed for data analysts, combining powerful utility functions for data manipulation, transformation, and analysis, along with ready-to-use React components for visualizing and interacting with data. Simplify workflows, accelerate analysis, and build stunning data-driven applications effortlessly.",
  "version": "0.0.0",
  "author": "Gabriel Juninho Paulista <gabrieltobing789@gmail.com>",
  "license": "MIT",
  "keywords": [
    "data-analytics",
    "data-analysis",
    "data-visualization",
    "react-components",
    "data-manipulation",
    "data-transformation",
    "analytics-utils",
    "data-utils",
    "charting",
    "data-cleaning",
    "react-data-tools",
    "data-analyst",
    "visualization-tools",
    "dashboard",
    "data-pipeline",
    "interactive-data",
    "react-visualization",
    "data-framework",
    "ETL",
    "data-processing"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "npm run format && npm run check-types && npm run lint && npm run check-format",
    "start": "tsup --watch",
    "check-format": "prettier --check .",
    "check-types": "tsc --pretty --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrieljuninho/datalyst.git"
  },
  "bugs": {
    "url": "https://github.com/gabrieljuninho/datalyst/issues"
  },
  "homepage": "https://github.com/gabrieljuninho/datalyst#readme",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/papaparse": "^5.3.15",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "dependencies": {
    "papaparse": "^5.4.1"
  }
}
